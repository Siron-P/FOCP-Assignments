    "num = int(input(\"Enter the required number of table (0-12): \"))\n",
    "if num>=0:\n",
    "    for i in range(num+1):\n",
    "        ans = i * 7\n",
    "        print(f\"{i} X 7 = {ans}\")\n",
    "\n",
    "else:\n",
    "    num = abs(num)\n",
    "    for i in range(num,-1,-1):\n",
    "        ans = i * 7\n",
    "        print(f\"{i} X 7 = {ans}\")"
   ]
