    "def decryption(encrypted,interval):\n",
    "    decrypted = encrypted[0::interval+1]\n",
    "    return decrypted\n",
    "\n",
    "encrypted = input(\"Enter the encrypted message : \")\n",
    "interval = int(input(\"Enter the interval used : \"))\n",
    "\n",
    "print(\"The decrypted message is \",decryption(encrypted,interval))"
