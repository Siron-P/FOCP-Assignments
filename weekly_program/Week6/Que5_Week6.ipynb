    "def hide_text(word):\n",
    "    hide = \"\"\n",
    "    for ch in word:\n",
    "        hide += ch + 'xy'\n",
    "\n",
    "    return hide\n",
    "\n",
    "text = input(\"Enter any word : \")\n",
    "print(\"Interval used : 2\")\n",
    "print(hide_text(text))"
