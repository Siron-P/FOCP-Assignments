   "source": [
    "def binary_base(num):\n",
    "    binary = bin(num)[2:]\n",
    "    return binary\n",
    "\n",
    "n = int(input(\"Enter any decimal number : \"))\n",
    "print(f\"The binary number of {n} is {binary_base(n)}.\")"
   ]
