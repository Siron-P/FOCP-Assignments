   "source": [
    "import sys\n",
    "\n",
    "if sys.platform.startswith(\"win\"):\n",
    "    name = \"Windows\"\n",
    "elif sys.platform.startswith(\"linux\"):\n",
    "    name = \"Linux\"\n",
    "elif sys.platform.startswith(\"darwin\"):\n",
    "    name = \"macOS\"\n",
    "else:\n",
    "    name = \"Unknown operating system\"\n",
    "\n",
    "print(f\"Operating system platform: {name}\")"
   ]
